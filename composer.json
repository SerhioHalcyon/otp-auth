{
    "name": "empat/otp",
    "description": "Sending otp by different providers",
    "keywords": ["laravel", "otp", "sms", "twilio", "turbosms", "notification"],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Serhii Babik",
            "email": "serhii.babik@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.10",
        "laravel-notification-channels/twilio": "^3.0.0",
        "laravel-notification-channels/turbosms": "^1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Empat\\Otp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Empat\\Otp\\OtpServiceProvider"
            ],
            "aliases": {
                "Otp": "Empat\\Otp\\Otp"
            }
        }
    },
    "version": "1.0.0"
}
